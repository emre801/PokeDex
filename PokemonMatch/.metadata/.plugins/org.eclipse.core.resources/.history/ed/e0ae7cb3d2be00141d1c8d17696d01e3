package com.johnerdo.battle;

import java.util.LinkedList;

import org.johnerdo.globalInfo.PokemonList;

import com.johnerdo.imageCompare.*;

public class Battle {

	LinkedList<String> pokemonOnScreen;
	public Battle(){
		setUp();
		printPokemon();
		PokemonList.setMapping();
	}
	public void setUp(){
		this.pokemonOnScreen = MatchingMethod.getPokemonNamesOnScreen();
	}
	public void printPokemon(){
		for(String name: pokemonOnScreen)
			System.out.println(name);
		System.out.println("OMG");
		MatchingMethod.copyGifs(pokemonOnScreen);
	}
	public static void main(String[] args){
		new Battle();
	}
}
